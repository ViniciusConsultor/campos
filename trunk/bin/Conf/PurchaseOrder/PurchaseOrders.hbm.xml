<?xml version="1.0" encoding="utf-8" ?> 
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.0" assembly="EzPos">
	<class name="EzPos.Model.PurchaseOrder,EzPos" table="[TPurchaseOrders]">
		<id name="PurchaseOrderID" column="PurchaseOrderID" unsaved-value="0" type="Int32" >
			<generator class="identity" />
		</id>
		<property name="PurchaseOrderNumber" column="PurchaseOrderNumber" />
    <property name="PurchaseOrderDate" column="PurchaseOrderDate" type="DateTime" />
    <property name="DeliveryDate" column="DeliveryDate" type="DateTime" />
    <property name="PaymentDate" column="PaymentDate" type="DateTime" />
    <property name="CurrencyID" column="CurrencyID" type="Int32" />
    <property name="ExchangeRate" column="ExchangeRate" type="float" />
    <property name="AmountCurrency" column="AmountCurrency" type="float" />
    <property name="AmountStandard" column="AmountStandard" type="float" />
    <property name="StatusID" column="StatusID" type="Int32" />
    <property name="SupplierID" column="SupplierID" type="Int32" />
    <property name="PaymentTermID" column="PaymentTermID" type="Int32" />
    <property name="Description" column="Description" />
    <property name="Discount" column="Discount" type="float" />
    <property name="AmountPaid" column="AmountPaid" type="float" />
    
    <many-to-one name="FKCurrency" column="CurrencyID" class="EzPos.Model.Currency, EzPos" insert="false" update="false"	/>
    <many-to-one name="FKPurchaseOrderStatus" column="StatusID" class="EzPos.Model.PurchaseOrderStatus, EzPos" insert="false" update="false"	/>
    <many-to-one name="FKSupplier" column="SupplierID" class="EzPos.Model.Supplier, EzPos" insert="false" update="false"	/>
    <many-to-one name="FKPaymentTerm" column="PaymentTermID" class="EzPos.Model.PaymentTerm, EzPos" insert="false" update="false"	/>    
  </class>
</hibernate-mapping>
		
	
		
		
		
	
		
		
		
